        android:name="com.example.hbapplicationgroupa.ui.BookingDetailsFragment"
        android:label="fragment_booking_details"
        tools:layout="@layout/fragment_booking_details" >
        <action
            android:id="@+id/action_bookingDetailsFragment_to_paymentCheckoutFragment"
            app:destination="@id/paymentCheckoutFragment" />
    </fragment>
    <fragment
        android:id="@+id/paymentCheckoutFragment"
        android:name="com.example.hbapplicationgroupa.ui.PaymentCheckoutFragment"
        android:label="fragment_payment_checkout"
        tools:layout="@layout/fragment_payment_checkout" >
        <action
            android:id="@+id/action_paymentCheckoutFragment_to_bookingConfirmationFragment"
            app:destination="@id/bookingConfirmationFragment" />
    </fragment>
    <fragment
        android:id="@+id/bookingConfirmationFragment"
        android:name="com.example.hbapplicationgroupa.ui.BookingConfirmationFragment"
        android:label="fragment_booking_confirmation"
        tools:layout="@layout/fragment_booking_confirmation" >
        <action
            android:id="@+id/action_bookingConfirmationFragment_to_wishListFragment"
            app:destination="@id/wishListFragment" />
    </fragment>
    <fragment
        android:id="@+id/wishListFragment"
        android:name="com.example.hbapplicationgroupa.ui.WishListFragment"
        android:label="fragment_wish_list"
        tools:layout="@layout/fragment_wish_list" >
        <action
            android:id="@+id/action_wishListFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
