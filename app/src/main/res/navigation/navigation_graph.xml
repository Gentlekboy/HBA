<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/splashScreenFragment">

    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.example.hbapplicationgroupa.ui.SplashScreenFragment"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreenFragment_to_onboarding01Fragment"
            app:destination="@id/onboarding01Fragment" />
    </fragment>
    <fragment
        android:id="@+id/onboarding01Fragment"
        android:name="com.example.hbapplicationgroupa.ui.Onboarding01Fragment"
        android:label="fragment_onboarding01"
        tools:layout="@layout/fragment_onboarding01" >
        <action
            android:id="@+id/action_onboarding01Fragment_to_onboarding02Fragment"
            app:destination="@id/onboarding02Fragment" />
    </fragment>
    <fragment
        android:id="@+id/onboarding02Fragment"
        android:name="com.example.hbapplicationgroupa.ui.Onboarding02Fragment"
        android:label="fragment_onboarding02"
